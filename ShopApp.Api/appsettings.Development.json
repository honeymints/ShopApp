{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Audience": "ShopApp",
    "ExpiryMinutes": 120,
    "Issuer": "ShopApp",
    "Secret": "secret key ShopApp for JWT Token"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=<ServerName>;Port=<PortNumber>;Database=<Your_Database>;;Username=<Your_Username>;Password=<Your_Password>"
  }
}
